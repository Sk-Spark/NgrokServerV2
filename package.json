    "@types/node-fetch": "^2.6.12",
    "@types/ws": "^8.18.1",
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.10"
  }
